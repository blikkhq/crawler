{
  "id": "crawler-backend",
  "kind": "Pod",
  "apiVersion": "v1beta1",
  "desiredState": {
    "manifest": {
      "version": "v1beta1",
      "id": "crawler-backend",
      "containers": [{
        "name": "blikk-crawler-backend",
        "image": "dennybritz/blikk-crawler",
        "command": "/blikk-crawler/crawler-backend/target/start",
        "ports": [{
          "containerPort": 8080,
          "hostPort": 8080
        }]
        // TODO: Add Splunk forwarder
      }]
    }
  },
  "labels": {
    "name": "crawler-backend"
  }
}